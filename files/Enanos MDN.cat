    <selectionEntry id="9610-40ce-15dc-f5b6" name="Barcatronante" hidden="false" collective="false" import="true" type="unit">
      <constraints>
        <constraint field="selections" scope="parent" value="1.0" percentValue="false" shared="true" includeChildSelections="false" includeChildForces="false" id="bda3-41e6-afb0-3c0b" type="max"/>
