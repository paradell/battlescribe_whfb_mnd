          </constraints>
          <profiles>
            <profile id="4f8f-4e2a-cc8f-2b80" name="Maestro de la hechicería" hidden="false" typeId="7e35-c20a-e9e9-6dad" typeName="Talismán">
              <characteristics>
                <characteristic name="Protección" typeId="9f41-c15e-1f5f-9d01">El Demonio sabe un hechizo más de los que le corresponden.</characteristic>
