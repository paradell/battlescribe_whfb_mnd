    <selectionEntryGroup id="144d-075f-eadb-39a2" name="Recompensas demoníacas Slaanesh (Demonios)" hidden="false" collective="false" import="true">
      <selectionEntries>
        <selectionEntry id="0891-5b57-8c83-02ff" name="Almizcle soporífero" hidden="false" collective="false" import="true" type="upgrade">
