      <rules>
        <rule id="6fb3-94fb-eba8-889f" name="Aparición del desierto" publicationId="9806-8ad8-97a2-86b5" page="31" hidden="false">
          <description>Apophas no puede unirse a unidades ni ser el General.</description>
        </rule>
        <rule id="a514-06c7-68f7-23da" name="Príncipe Escarabajo" publicationId="9806-8ad8-97a2-86b5" page="31" hidden="false">
