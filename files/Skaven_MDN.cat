        <selectionEntry id="ed3d-7529-4ec1-97a2" name="Pergamino de tormenta de disformidad" hidden="false" collective="false" import="true" type="upgrade">
          <constraints>
            <constraint field="selections" scope="roster" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="false" id="29ee-e8d2-faf7-86ae" type="max"/>
