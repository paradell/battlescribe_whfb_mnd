    <selectionEntry id="8afd-2849-5ae9-9154" name="Nurglitch" publicationId="c220-b30e-7003-9bc3" page="39" hidden="false" collective="false" import="true" type="unit">
      <comment>Archiseñor de la plaga</comment>
      <profiles>
        <profile id="90c0-dc3c-b5ca-8756" name="Nurglitch" hidden="false" typeId="1025-8460-b30f-58db" typeName="Modelo">
          <characteristics>
            <characteristic name="M" typeId="2d56-9cad-9242-37bd">12</characteristic>
            <characteristic name="HA" typeId="c87b-b820-6a47-1354">6</characteristic>
            <characteristic name="HP" typeId="8ab7-c800-6644-f5ec">3</characteristic>
