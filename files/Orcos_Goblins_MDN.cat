          <profiles>
            <profile id="7308-f5f7-ea52-3ef3" name="Fanático" hidden="false" typeId="1025-8460-b30f-58db" typeName="Modelo">
              <characteristics>
                <characteristic name="M" typeId="2d56-9cad-9242-37bd">5D6</characteristic>
                <characteristic name="HA" typeId="c87b-b820-6a47-1354"/>
