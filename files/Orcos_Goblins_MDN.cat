    <selectionEntry id="00a0-1568-51c7-0051" name="Badruk Machakakráneoz" publicationId="1961-98cd-e2f9-7572" page="49" hidden="false" collective="false" import="false" type="model">
      <constraints>
        <constraint field="selections" scope="roster" value="1.0" percentValue="false" shared="true" includeChildSelections="false" includeChildForces="false" id="a9ff-e1c4-aac4-e22d" type="max"/>
